<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="User" table="user">
  <id name="u_id">
   <generator class="increment"></generator>
  </id>
  <property name="u_first_name" ></property>
  <property name="u_last_name" ></property>
  <property name="u_add1"></property>
  <property name="u_add2" ></property>
  <property name="u_dob" ></property>
  <property name="u_email"></property>
  <property name="u_password" ></property>
  <property name="u_city" ></property>
  <property name="u_mobile"></property>
</class>

</hibernate-mapping>